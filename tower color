local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Tower of Colors! üåà",
    Icon = 0,
    LoadingTitle = "Tower of Colors! üåà",
    LoadingSubtitle = "https://discord.gg/5Y4pyevZZT",
    Theme = "Default",
 
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,
 
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil,
       FileName = "Frusteddy hub"
    },
 
    Discord = {
       Enabled = true,
       Invite = "5Y4pyevZZT",
       RememberJoins = true
    },
 
    KeySystem = false,
    KeySettings = {
       Title = "Untitled",
       Subtitle = "Key System",
       Note = "No method of obtaining the key is provided",
       FileName = "Key",
       SaveKey = true,
       GrabKeyFromSite = false,
       Key = {"Hello"}
    }
})

local Tab = Window:CreateTab("Teleport", 4483362458)

Rayfield:Notify({
    Title = "Script Executado!",
    Content = "Frusteddy Hub",
    Duration = 6.5,
    Image = 4483362458,
})

local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()

local AutoFarmEnabled = false

local Toggle = Tab:CreateToggle({
    Name = "Auto Farm",
    CurrentValue = false,
    Flag = "Auto farm",
    Callback = function(Value)
        AutoFarmEnabled = Value
        if AutoFarmEnabled then
            task.spawn(function()
                while AutoFarmEnabled do
                    local character = Player.Character
                    if character then
                        local rootPart = character:FindFirstChild("HumanoidRootPart")
                        if rootPart then
                            rootPart.CFrame = CFrame.new(-3.49996948, 430.5, -68.5)
                        end
                    end
                    wait(3)
                end
            end)
        end
    end
})

local function switchToggle()
    AutoFarmEnabled = false
    Toggle:Set(false) -- Desativa o toggle quando o usu√°rio clica
end

Mouse.Button1Down:Connect(switchToggle)

local Tab = Window:CreateTab("AntiAfk", 4483362458)

local Button = Tab:CreateButton({
   Name = "AntiAfk",
   Callback = function()
   local VirtualInputManager = game:GetService("VirtualInputManager")

task.spawn(function()
    while true do
        -- Simula um clique do bot√£o esquerdo do mouse
        VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
        VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
        
        print("üñ±Ô∏è(Anti-AFK ativo)")
        
        -- Aguarda 1 minuto antes de clicar novamente
        task.wait(60)
    end
end)

   end,
})

local Tab = Window:CreateTab("Coin Farm", 4483362458)

local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()

local AutoFarmEnabled = false

local Toggle = Tab:CreateToggle({
    Name = "Auto FarmüíµCOINS",
    CurrentValue = false,
    Flag = "Auto farm",
    Callback = function(Value)
        AutoFarmEnabled = Value
        if AutoFarmEnabled then
            task.spawn(function()
                while AutoFarmEnabled do
                    local character = Player.Character
                    if character then
                        local rootPart = character:FindFirstChild("HumanoidRootPart")
                        if rootPart then
                            rootPart.CFrame = CFrame.new(-3.49996948, 430.5, -68.5)
                        end
                    end
                    wait(3)
                end
            end)
        end
    end
})

